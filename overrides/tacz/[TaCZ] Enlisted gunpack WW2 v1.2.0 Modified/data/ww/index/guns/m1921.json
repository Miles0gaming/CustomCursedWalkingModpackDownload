{
    // 枪械的显示名称，支持本地化
    "name": "M21/28 Thomspon",
    // 枪械效果文件
    "display": "ww:m1921_display",
    // 枪械参数文件
    "data": "ww:m1921_data",
    // 枪械文本提示
    "tooltip": "Chicago Typewriter",
    // 枪械种类，用于创造标签栏归类。
    "type": "smg",
    // 枪械的物品类型，决定枪械的逻辑，如开火、装填等，默认是 modern_kinetic (现代动能枪械)
    "item_type": "modern_kinetic",
    // 排序数，决定了在创造模式标签页中的顺序
    // 数字越小，优先级越高
    "sort": 1
}